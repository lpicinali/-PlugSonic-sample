  "scripts": {
    "start": "NODE_PATH=src node scripts/start.js",
    "build": "NODE_PATH=src node scripts/build.js",
    "test": "NODE_PATH=src NODE_ENV=test mocha --require babel-core/register src/**/*-spec.js",
    "test:watch": "NODE_PATH=src NODE_ENV=test mocha --watch --require babel-core/register src/**/*-spec.js"
  },
  "babel": {
    "presets": [
      "react-app"
